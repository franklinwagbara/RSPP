            $('#addbtn').click(function (e) {
                e.preventDefault();
                var agencyID = Number($("#AgencyId").val());
                debugger;
                 if (agencyID === 3) {
                    var StatusOfTerminal = $("#StatusOfTerminal").val();
                    var portoffdockCargoType = $("#portoffdockCargoType").val();
